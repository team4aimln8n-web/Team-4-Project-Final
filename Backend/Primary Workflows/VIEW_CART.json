    {
      "parameters": {
        "jsCode": "const cartItems = $input.all();\nconst cartId = $('Get User Cart').first().json.id;\n\nconst items = cartItems.map(item => {\n  const quantity = item.json.quantity;\n  const price = item.json.price;\n  const subtotal = price * quantity;\n  \n  return {\n    cart_item_id: item.json.id,       // âœ… ADD THIS\n    product_id: item.json.product_id,\n    name: item.json.name,\n    price: price,\n    quantity: quantity,\n    subtotal: subtotal,\n    image_url: item.json.image_url\n  };\n});\n\nconst total = items.reduce((sum, item) => sum + item.subtotal, 0);\n\nreturn [{\n  json: {\n    cart_id: cartId,\n    items: items,\n    total: total\n  }\n}];\n"
      },
